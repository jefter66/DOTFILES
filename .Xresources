! st terminal stuff
st*locale: true
st*scrollBar:false
st*alpha:0.95

*faceName:Liberation Mono:pixelsize=15
Alt <Key>C: copy-selection(CLIPBOARD) \n\
XTerm*faceSize: 10

*reverseVideo: true


*.cursorColor:  #2626F6
XTerm.vt100.locale: true
XTerm.vt100.utf8: true

! Making tab works
XTerm.vt100.metaSendsEscape: true

XTerm.vt100.backarrowKey: false
XTerm.ttyModes: erase ^?

XTerm.vt100.translations: #override \n\
    Ctrl <Key>M: maximize() \n\
    Ctrl <Key>R: restore()

!!black
!*color0:            #393939
!*color0:            #DD4B4B
!*color8:            #474747
!red
*color1:            #af005f
*color9:            #ff6c5c
!green
*color2:            #519f50
*color10:           #a5c261
!yellow
*color3:            #cc7833
*color11:           #bc9458
!blue
*color4:            #1C5F87
*color12:           #3288C6
!*magenta
*color5:            #5f1c5f
*color13:           #A617DE
!*cyan
