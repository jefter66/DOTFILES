st*termName: xterm-256color
st*locale: true
st*scrollBar:false
st*geometry: 80x32
st*faceName:liberation mono:pixelsize=10
!*alpha:0.9


Alt <Key>C: copy-selection(CLIPBOARD) \n\
Alt <Key>V: insert-selection(CLIPBOARD)

*reverseVideo: true
!black
*color0:            #393939
*color8:            #474747
!red
*color1:            #da4939
*color9:            #ff6c5c
!green
*color2:            #519f50
*color10:           #a5c261
!yellow
*color3:            #cc7833
*color11:           #bc9458
!blue
*color4:            #3288C6
*color12:           #3288C6
!*magenta
*color5:            #E10778
*color13:           #A617DE
!*cyan
*color6:            #435d75
*color14:           #6e98a4
!*white
*color7:            #dddddd
*color15:           #dddddd
